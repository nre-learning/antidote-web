{
  "name": "antidote-web",
  "version": "1.0.0",
  "description": "scripts and components for use in any app integrating with an Antidote project backend",
  "main": "js/antidote.js",
  "dependencies": {
    "antidote-localizations": "nre-learning/antidote-localizations",
    "antidote-ui-components": "nre-learning/antidote-ui-components",
    "nre-styles": "nre-learning/nre-styles"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "rollup": "^1.27.12",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.3"
  },
  "scripts": {
    "dev": "CLEAN_INSTALL=true COMPOSE_UP=true sh scripts/dev.sh",
    "dev:up": "COMPOSE_UP=true sh scripts/dev.sh",
    "dev:down": "COMPOSE_DOWN=true sh scripts/dev.sh",
    "dev-restart": "COMPOSE_RESTART=true sh scripts/dev.sh",
    "build": "npx rollup -c",
    "watch": "npx rollup -cw",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nre-learning/antidote-web.git"
  },
  "keywords": [
    "antidote",
    "syringe"
  ],
  "author": "Nils Lundquist",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nre-learning/antidote-web/issues"
  },
  "homepage": "https://github.com/nre-learning/antidote-web#readme"
}
